--------------------------------------------------------
--  DDL for Trigger TRG_FAQ_ID
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "ADMIN"."TRG_FAQ_ID" 
BEFORE INSERT ON TBL_FAQ
FOR EACH ROW
BEGIN
    :NEW.faq_id := faq_seq.NEXTVAL;
END;

/
ALTER TRIGGER "ADMIN"."TRG_FAQ_ID" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TRG_NOTICE_ID
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "ADMIN"."TRG_NOTICE_ID" 
BEFORE INSERT ON TBL_NOTICE
FOR EACH ROW
BEGIN
    :NEW.notice_id := notice_seq.NEXTVAL;
END;

/
ALTER TRIGGER "ADMIN"."TRG_NOTICE_ID" ENABLE;
